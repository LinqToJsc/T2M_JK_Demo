@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>金科服务管理系统</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Scripts/t2m_adminlte/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Scripts/t2m_adminlte/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Scripts/t2m_adminlte/bower_components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Scripts/t2m_adminlte/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
    folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Scripts/t2m_adminlte/dist/css/skins/_all-skins.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <link href="~/Content/JinKeYGY.css" rel="stylesheet"/>
    <style>
        .content-wrapper {
            background: url(/Content/login_bg.png) no-repeat;
            background-size: cover;
            min-height: 75% !important;
        }
        .login-box-body {
            opacity: 0.9;
            border-radius: 4px;
            padding-top: 40px;
            padding-bottom: 40px;
        }
        .has-feedback > input {
            border-radius: 4px;
        }

        .jk-slogan {
            position: relative;height: 225px;
        }
        .jk-slogan-div {
            padding-bottom: 10px;position: absolute;bottom: 0;
        }
        .jk-slogan-title {
            font-family: JinKeYGY;font-size: 36px;font-weight: bold;color: white;
        }
        .jk-slogan-remark {
            padding-left: 23px;padding-right: 23px;color: white;width: 368px;margin-top: 10px;
        }

    </style>
</head>


@*<div class="content-wrapper" style="background-color: #e6e9ec;">
    <div class="container">

    </div>
</div>*@
<!-- ADD THE CLASS layout-top-nav TO REMOVE THE SIDEBAR. -->
<body class="hold-transition  layout-top-nav">
    <div class="wrapper">
        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        
                        <a href="/Home/Index" class="logo">
                            <!-- mini logo for sidebar mini 50x50 pixels -->
                            <span class="logo-mini"><b>金科</b></span>
                            <!-- logo for regular state and mobile devices -->
                            @*<span class="logo-lg"><b>金科</b>服务管理系统</span>*@
                            <span class="logo-lg"><img src="~/Content/logo_login.png" /></span>
                        </a>

                    </div>

                </div>
                <!-- /.container-fluid -->
            </nav>
        </header>
        <!-- Full Width Column -->
        <div class="content-wrapper" style="background-color: #e6e9ec;">
            <div class="container">
                <section class="content" style="margin-top: 12%;">
                    <div class="row">
                        <div class="col-md-5 jk-slogan">

                            <div class="jk-slogan-div">
                                <span class="jk-slogan-title">“让工作效率飞起来”</span>
                                <div class="jk-slogan-remark">
                                    集成企业沟通、协作、管理、数据、文化建设为一体的办公平台，多终端登录，随时随地处理工作事物，高效提升办公效率。
                                </div>
                            </div>

                        </div>



                        <div class="col-md-3">
                            
                        </div>
                        <div class="col-md-4" style="width: 30%;">
                            <div class="login-box-body">
                                <form action="../../index2.html" method="post">
                                    <div class="form-group has-feedback">
                                        <input type="text" name="staffId" id="staffId" class="form-control" style="padding-left: 35px; padding-right: 0;" placeholder="请输入员工编号">
                                        <span class="glyphicon glyphicon-user form-control-feedback" style="left: 0;"></span>
                                    </div>
                                    <div class="form-group has-feedback">
                                        <input type="password" name="pwd" id="pwd"  class="form-control" style="padding-left: 35px; padding-right: 0;"  placeholder="请输入员工密码">
                                        <span class="glyphicon glyphicon-lock form-control-feedback"  style="left: 0; color: #c0c8cf;"></span>
                                    </div>
                                    <div class="row" style="margin-top: 36px;">
                                        <div class="col-xs-12">
                                            <button type="submit" class="btn btn-primary btn-block" style="">登 录</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>

                    </div>
                </section>
                
            </div>
            
        </div>
        
        <footer class="main-footer">
            <div class="container text-center" style="color: #999;">
               
                <strong>Copyright &copy; 2018金科服务 版权所有。</strong> 渝ICP备XXXXXXXX号
                
            </div>
            <!-- /.container -->
        </footer>
    </div>
    
    <!-- jQuery 3 -->
    <script src="~/Scripts/t2m_adminlte/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Scripts/t2m_adminlte/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
</body>
</html>
